{
  "name": "@react-native-oh-tpl/react-native-haptic-feedback",
  "version": "2.2.0-0.0.1",
  "description": "Basic haptic feedback for iOS and android",
  "license": "MIT",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "typings": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "nativePackage": true,
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bob": "bob build",
    "prepare": "npm run bob"
  },
  "keywords": [
    "react-native",
    "haptic",
    "haptic-feedback",
    "android",
    "ios",
    "native",
    "feedback",
    "harmony"
  ],
  "author": "Michael Kuczera",
  "peerDependencies": {
    "react-native": ">=0.60.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-native-oh-library/react-native-haptic-feedback.git"
  },
  "homepage": "https://github.com/mkuczera/react-native-haptic-feedback",
  "readme": "https://github.com/mkuczera/react-native-haptic-feedback#readme",
  "bugs": {
    "url": "https://github.com/mkuczera/react-native-haptic-feedback/issues"
  },
  "codegenConfig": {
    "name": "RNHapticFeedbackSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.mkuczera"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/runtime": "^7.21.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.71.3",
    "prettier": "^2.8.5",
    "typescript": "^5.0.2",
    "react-native-builder-bob": "^0.20.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
        "commonjs",
        "module",
        [
            "typescript",
            {
                "project": "tsconfig.build.json" 
            }
        ]
    ]
},
  "harmony": {
    "alias": "react-native-haptic-feedback",
   "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  }
}
